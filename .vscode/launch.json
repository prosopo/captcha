{
	"version": "1.0.0",
	"configurations": [
		{
			"name": "tsx",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
			"program": "${workspaceRoot}/packages/util/solverService.ts",
			"args": [
				"version"
			],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			]
		}
	]
}
