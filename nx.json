{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"namedInputs": {
		"default": [
			"{projectRoot}/**",
			"!{projectRoot}/dist/**"
		]
	},
	"namedOutputs": {
		"dist": [
			"{projectRoot}/dist"
		]
	},
	"targetDefaults": {
		"build": {
			"dependsOn": [
				"^{target}"
			],
			"cache": true
		},
		"build:cjs": {
			"dependsOn": [
				"^{target}"
			],
			"cache": true
		},
		"bundle": {
			"dependsOn": [
				"^build",
				"^{target}"
			],
			"cache": true
		},
		"bundle:webpack": {
			"dependsOn": [
				"^build:cjs",
				"^{target}"
			],
			"cache": true
		},
		"test": {
			"cache": true
		},
		"typecheck": {
			"dependsOn": [
				"^{target}"
			],
			"cache": true
		},
		"lint": {
			"cache": true
		}
	},
	"defaultBase": "main",
	"release": {
		"projects": [
			"*"
		],
		"versionPlans": true,
		"projectsRelationship": "independent",
		"changelog": {
			"projectChangelogs": true,
			"automaticFromRef": true
		}
	},
	"parallel": 8,
	"pluginsConfig": {
		"@nx/js": {
			"projectsAffectedByDependencyUpdates": "auto"
		}
	},
	"plugin": "@nx/js/typescript",
	"options": {
		"typecheck": {
			"targetName": "typecheck"
		}
	},
	"neverConnectToCloud": true,
	"sync": {
		"applyChanges": true
	},
	"useDaemonProcess": false
}