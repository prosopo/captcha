{
	"name": "@prosopo/react-procaptcha-wrapper",
	"version": "1.1.42",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV",
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo"
	},
	"dependencies": {
		"@prosopo/procaptcha-wrapper": "2.6.44"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@prosopo/config": "3.1.27",
		"@prosopo/procaptcha-integration-build-config": "1.1.27",
		"@types/node": "22.10.2",
		"@vitejs/plugin-react": "5.1.0",
		"del-cli": "6.0.0"
	},
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
