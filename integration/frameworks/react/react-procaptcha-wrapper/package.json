{
	"name": "@prosopo/react-procaptcha-wrapper",
	"version": "1.1.11",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV",
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo"
	},
	"dependencies": {
		"@prosopo/procaptcha-wrapper": "2.6.13",
		"@prosopo/config": "3.1.5"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@prosopo/procaptcha-integration-build-config": "^1.1.5",
		"@types/node": "^20.0.0",
		"@vitejs/plugin-react": "^4.0.0",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
