{
	"name": "@prosopo/svelte-procaptcha-wrapper",
	"version": "1.1.43",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"svpkg": "svelte-package -i ./src -o ./dist",
		"build": "npm run svpkg && del-cli --verbose ./.svelte-kit",
		"dev": "npm run svpkg -- -w",
		"test": "NODE_ENV=${NODE_ENV:-test} vitest run --config ./vite.test.config.ts"
	},
	"dependencies": {
		"@prosopo/procaptcha-wrapper": "2.6.45"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@prosopo/config": "3.2.1",
		"@sveltejs/package": "2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/ui": "3.2.4",
		"del-cli": "6.0.0",
		"jsdom": "25.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "4.2.2",
		"vitest": "3.2.4"
	},
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
