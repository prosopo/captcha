{
	"name": "@prosopo/svelte-procaptcha-wrapper",
	"version": "1.1.1",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"test": "echo no tests",
		"clean": "del-cli ./dist && del-cli ./.svelte-kit",
		"svpkg": "svelte-package -i ./src -o ./dist",
