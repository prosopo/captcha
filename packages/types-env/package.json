  "dependencies": {
    "@polkadot/api": "10.13.1",
    "@polkadot/keyring": "12.6.2",
    "@polkadot/rpc-provider": "10.13.1",
    "@prosopo/contract": "2.0.2",
    "@prosopo/common": "2.0.2",
    "@prosopo/types": "2.0.2",
    "@prosopo/types-database": "2.0.2"
  },
  "devDependencies": {
    "tslib": "2.6.2",
