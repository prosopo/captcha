{
	"name": "@prosopo/types-env",
	"version": "2.7.4",
	"description": "Types for prosopo environment",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"test": "echo \"No test specified\"",
		"clean": "tsc --build --clean",
		"build:tsc": "tsc --build",
		"build:bundle": "NODE_ENV=${NODE_ENV:-production}; vite --mode=$NODE_ENV --config vite.cjs.config.ts build",
		"build": "run-s build:tsc build:bundle"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/captcha.git"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@prosopo/common": "3.0.0",
		"@prosopo/types": "3.0.0",
		"@prosopo/types-database": "3.0.0",
		"express": "4.21.2"
	},
	"devDependencies": {
		"@prosopo/config": "3.0.0",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "2.1.0",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
		"typescript": "5.6.2",
		"vite": "6.2.3",
		"vitest": "3.0.9"
	},
	"sideEffects": false
}