{
    "name": "@prosopo/types",
    "version": "0.3.37",
    "description": "Types for prosopo TypeScript packages",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "test": "echo \"No test specified\"",
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "eslint": "npx eslint . --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore",
        "eslint:fix": "npm run eslint -- --fix",
        "prettier": "npx prettier . --check --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "npm run eslint && npm run prettier",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/prosopo/types.git"
    },
    "author": "Prosopo Limited",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "dependencies": {
        "@polkadot/api": "10.11.2",
        "@polkadot/api-contract": "10.11.2",
        "@polkadot/types": "10.11.2",
        "@polkadot/types-codec": "10.11.2",
        "@prosopo/common": "0.3.37",
        "@prosopo/captcha-contract": "0.3.37",
        "consola": "^3.2.3",
        "zod": "^3.22.3"
    },
    "devDependencies": {
        "@types/node": "^18.0.6",
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "sideEffects": false
}
