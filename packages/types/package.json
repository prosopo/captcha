{
	"name": "@prosopo/types",
	"version": "3.8.3",
	"description": "Types for prosopo TypeScript packages",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "npm run build:cross-env -- --mode ${NODE_ENV:-development}",
		"build:cross-env": "vite build --config vite.esm.config.ts",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/types.git"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@polkadot/extension-inject": "0.62.3",
		"@polkadot/types": "16.4.9",
		"@polkadot/util": "13.5.7",
		"@prosopo/locale": "3.1.28",
		"@prosopo/util": "3.2.5",
		"@prosopo/util-crypto": "13.5.29",
		"ip-address": "10.0.1",
		"scale-ts": "1.6.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@prosopo/config": "3.3.0",
		"@types/node": "22.5.5",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.4.1",
		"vitest": "3.2.4"
	},
	"sideEffects": false
}
