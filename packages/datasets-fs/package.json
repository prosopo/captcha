    "@prosopo/types": "2.0.2",
    "@prosopo/util": "2.0.2",
    "bcrypt": "^5.1.0",
    "cli-progress": "^3.12.0",
    "sharp": "^0.32.1",
    "vitest": "^1.3.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@prosopo/config": "2.0.2",
