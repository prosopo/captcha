{
    "name": "@prosopo/env",
    "version": "1.0.0",
    "description": "Path env prosopo environment",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "test": "echo \"No test specified\"",
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "eslint": "npx eslint . --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore --quiet",
        "eslint:fix": "npm run eslint -- --fix",
        "prettier": "npx prettier . --check --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "npm run eslint && npm run prettier",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix"
    },
    "dependencies": {
        "@polkadot/api": "10.13.1",
        "@polkadot/keyring": "12.6.2",
        "@polkadot/rpc-provider": "10.13.1",
        "@polkadot/util": "12.6.2",
        "@polkadot/util-crypto": "12.6.2",
        "@prosopo/captcha-contract": "1.0.0",
        "@prosopo/common": "1.0.0",
        "@prosopo/contract": "1.0.0",
        "@prosopo/database": "1.0.0",
        "@prosopo/types": "1.0.0",
        "@prosopo/types-database": "1.0.0",
        "@prosopo/types-env": "1.0.0",
        "@prosopo/util": "1.0.0"
    },
    "overrides": {
        "@polkadot/keyring": "12.6.2"
    },
    "devDependencies": {
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "author": "Prosopo",
    "license": "Apache-2.0",
    "sideEffects": false
}
