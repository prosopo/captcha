		"@prosopo/common": "2.0.0",
		"@prosopo/contract": "2.0.0",
		"@prosopo/database": "2.0.0",
		"@prosopo/types": "2.0.0",
		"@prosopo/types-database": "2.0.0",
		"@prosopo/types-env": "2.0.0",
		"@prosopo/util": "2.0.0"
	},
	"overrides": {
		"@polkadot/keyring": "12.6.2"
	},
	"devDependencies": {
		"@prosopo/config": "2.0.0",
		"tslib": "2.6.2",
