{
    "name": "@prosopo/env",
    "version": "0.2.0",
    "description": "Path env prosopo environment",
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix --config ../../.eslintrc.js"
    },
    "dependencies": {
        "@polkadot/util-crypto": "12.3.2",
        "@prosopo/common": "0.2.0",
        "@prosopo/contract": "0.2.0",
        "@prosopo/database": "0.2.0",
        "@prosopo/types": "0.2.0",
        "@prosopo/types-database": "0.2.0",
        "@prosopo/types-env": "0.2.0",
        "@prosopo/util": "0.2.0",
        "dotenv": "^16.0.1"
    },
    "devDependencies": {
        "tslib": "^2.6.2",
        "typescript": "^5.1.6"
    },
    "author": "Prosopo",
    "license": "Apache-2.0",
    "sideEffects": false
}
