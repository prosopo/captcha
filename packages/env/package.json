{
	"name": "@prosopo/env",
	"version": "2.0.0",
	"description": "Path env prosopo environment",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=20",
		"npm": ">=9"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
