{
	"name": "@prosopo/cli",
	"version": "3.5.30",
	"description": "CLI for Prosopo Provider",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "npm run build:cross-env -- --mode ${NODE_ENV:-development}",
		"build:cross-env": "vite build --config vite.esm.config.ts",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"test": "NODE_ENV=${NODE_ENV:-test}; npx vitest run --config ./vite.test.config.ts",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV",
		"cli": "node ./dist/cli.js",
		"start": "node ./dist/cli.js --api",
		"start:admin": "node ./dist/cli.js --api --adminApi",
		"start:dev": "node ./dist/cli.js --api --dev"
	},
	"dependencies": {
		"@polkadot/types-codec": "16.4.9",
		"@prosopo/api": "3.1.46",
		"@prosopo/common": "3.1.28",
		"@prosopo/dotenv": "3.0.33",
		"@prosopo/env": "3.3.1",
		"@prosopo/keyring": "2.9.5",
		"@prosopo/locale": "3.1.28",
		"@prosopo/provider": "3.18.1",
		"@prosopo/types": "3.9.0",
		"@prosopo/util": "3.2.5",
		"@prosopo/workspace": "3.1.20",
		"cron-parser": "4.9.0",
		"dotenv": "16.4.5",
		"yargs": "17.7.2",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@prosopo/config": "3.3.0",
		"@polkadot-api/substrate-client": "0.1.4",
		"@types/cors": "2.8.17",
		"@types/node": "22.10.2",
		"@types/yargs": "17.0.33",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"express": "4.21.2",
		"npm-run-all": "4.1.5",
		"smoldot": "2.0.26",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.4.1",
		"vitest": "3.2.4"
	},
	"author": "Prosopo",
	"license": "Apache-2.0",
	"sideEffects": false
}
