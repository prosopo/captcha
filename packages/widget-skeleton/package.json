{
	"name": "@prosopo/widget-skeleton",
	"version": "2.6.0",
	"description": "Non business logic utilities for web applications",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"test": "echo \"No test specified\"",
		"clean": "tsc --build --clean",
