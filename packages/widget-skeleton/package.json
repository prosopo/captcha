		"bundle": "NODE_ENV=${NODE_ENV:-production}; vite --mode=$NODE_ENV --config vite.cjs.config.ts build"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/types.git"
	},
