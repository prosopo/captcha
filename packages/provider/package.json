{
	"name": "@prosopo/provider",
	"version": "3.0.0",
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"main": "./dist/index.js",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "tsc --build --clean",
		"build": "tsc --build",
		"bundle": "NODE_ENV=${NODE_ENV:-production}; vite --mode=$NODE_ENV --config vite.cjs.config.ts build",
		"test": "NODE_ENV=${NODE_ENV:-test}; npx vitest run --config ./vite.test.config.ts",
		"mnemonic": "tsx ./scripts/generateMnemonic.ts"
	},
	"dependencies": {
		"@noble/hashes": "1.5.0",
		"@polkadot/util": "12.6.2",
		"@prosopo/api-express-router": "3.0.0",
		"@prosopo/api-route": "2.6.5",
		"@prosopo/common": "3.0.0",
		"@prosopo/config": "3.0.0",
		"@prosopo/database": "3.0.0",
		"@prosopo/datasets": "3.0.0",
		"@prosopo/env": "3.0.0",
		"@prosopo/keyring": "2.8.3",
		"@prosopo/types": "3.0.0",
		"@prosopo/types-database": "3.0.0",
		"@prosopo/types-env": "2.7.4",
		"@prosopo/user-access-policy": "3.0.0",
		"@prosopo/util": "3.0.0",
		"@prosopo/util-crypto": "13.5.1",
		"cron": "3.1.7",
		"express": "4.21.2",
		"ip-address": "10.0.1",
		"node-fetch": "3.3.2",
		"read-tls-client-hello": "1.1.0",
		"uuid": "10.0.0",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@types/node": "22.5.5",
		"@types/uuid": "10.0.0",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"dotenv": "16.4.5",
		"npm-run-all": "2.1.0",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
		"typescript": "5.6.2",
		"vite": "6.2.3",
		"vitest": "3.0.9"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/provider.git"
	},
	"description": "The easiest way to deploy the Prosopo contract and run the Provider node is via the [captcha repository](https://github.com/prosopo/captcha/).",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}