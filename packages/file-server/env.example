PROSOPO_FILE_SERVER_REMOTES="http://localhost:9233" # list of remote servers to proxy requests to (can be JSON array)
PROSOPO_FILE_SERVER_PORT=9232 # port to run the server on
PROSOPO_FILE_SERVER_RESIZE=128 # resize images to this size (in pixels)
PROSOPO_FILE_SERVER_PATHS="/path/to/your/images" # list of paths containing images (can be JSON array)
PROSOPO_LOG_LEVEL=info
