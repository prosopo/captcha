{
	"name": "@prosopo/file-server",
	"version": "2.1.0",
	"description": "Simple static file server",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=20",
		"npm": ">=9"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"test": "echo \"No test specified\"",
		"clean": "tsc --build --clean",
		"build": "tsc --build --verbose",
		"build:cjs": "npx vite --config vite.cjs.config.ts build",
		"start": "node ./dist/index.js"
	},
	"dependencies": {
		"@prosopo/util": "2.1.0",
		"dotenv": "16.4.5",
		"express": "4.21.0",
		"node-fetch": "3.3.2",
		"sharp": "0.33.5"
	},
	"devDependencies": {
		"@prosopo/config": "2.1.0",
		"@types/express": "4.17.21",
		"@types/node": "22.5.5",
		"@vitest/coverage-v8": "2.1.1",
		"concurrently": "9.0.1",
		"npm-run-all": "4.1.5",
		"rimraf": "6.0.1",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
		"typescript": "5.6.2",
		"vite": "5.4.6",
		"vitest": "2.1.1"
	},
	"author": "Prosopo",
	"license": "Apache-2.0",
	"sideEffects": false
}
