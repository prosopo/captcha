{
	"name": "@prosopo/keyring",
	"version": "2.8.30",
	"description": "Keypair generator for Prosopo",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"test": "NODE_ENV=${NODE_ENV:-test}; npx vitest run --config ./vite.test.config.ts --mode $NODE_ENV"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/types.git"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@polkadot/util": "12.6.2",
		"@polkadot/util-crypto": "12.6.2",
		"@prosopo/common": "3.1.20",
		"@prosopo/config": "3.1.20",
		"@prosopo/types": "3.5.6",
		"@prosopo/util": "3.1.5",
		"@prosopo/util-crypto": "13.5.22",
		"dotenv": "16.4.5"
	},
	"devDependencies": {
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"sideEffects": false
}
