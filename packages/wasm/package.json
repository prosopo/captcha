{
    "name": "@prosopo/wasm",
    "version": "0.2.33",
    "description": "Simple static file server",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "build": "cd pkg2 && npm run build && cd .. && npx wasm-pack build && npx vite build"
    },
    "dependencies": {
        "dotenv": "^16.0.1",
        "express": "^4.18.2",
        "node-fetch": "^3.3.2",
        "sharp": "^0.32.4",
        "vite-plugin-top-level-await": "^1.4.1",
        "vite-plugin-wasm": "^3.3.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.5.9",
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "author": "Prosopo",
    "license": "Apache-2.0",
    "sideEffects": false
}