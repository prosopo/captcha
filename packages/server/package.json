{
	"name": "@prosopo/server",
	"version": "2.9.59",
	"description": "NodeJS package for server side communication with the prosopo captcha client",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "npm run build:cross-env -- --mode ${NODE_ENV:-development}",
		"build:cross-env": "vite build --config vite.esm.config.ts",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prosopo/captcha.git"
	},
	"author": "PROSOPO LIMITED",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false,
	"dependencies": {
		"@prosopo/api": "3.1.48",
		"@prosopo/common": "3.1.28",
		"@prosopo/keyring": "2.9.7",
		"@prosopo/load-balancer": "2.8.24",
		"@prosopo/types": "3.10.1",
		"@prosopo/util": "3.2.5",
		"i18next": "24.1.0"
	},
	"devDependencies": {
		"@prosopo/config": "3.3.0",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.4.1",
		"vitest": "3.2.4"
	}
}
