{
    "name": "@prosopo/server",
    "version": "0.2.4",
    "description": "NodeJS package for server side communication with the prosopo captcha client",
    "main": "./dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=16",
        "npm": "8.9"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix --config ../../.eslintrc.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prosopo/captcha.git"
    },
    "author": "PROSOPO LIMITED",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "sideEffects": false,
    "dependencies": {
        "@polkadot/api": "10.9.1",
        "@polkadot/keyring": "12.3.2",
        "@polkadot/rpc-provider": "10.9.1",
        "@polkadot/types": "10.9.1",
        "@prosopo/api": "0.2.4",
        "@prosopo/contract": "0.2.4",
        "@prosopo/procaptcha": "0.2.4",
        "@prosopo/captcha-contract": "^0.2.2",
        "@prosopo/types": "0.2.4"
    },
    "devDependencies": {
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    }
}
