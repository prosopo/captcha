		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@emotion/react": "11.13.3",
		"@emotion/styled": "11.13.0",
		"react": "18.3.1"
	},
	"devDependencies": {
		"@prosopo/config": "2.1.2",
		"@vitest/coverage-v8": "2.1.1",
		"concurrently": "9.0.1",
		"npm-run-all": "4.1.5",
		"del-cli": "6.0.0",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
