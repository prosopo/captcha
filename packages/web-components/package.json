		"@emotion/styled": "^11.11.0",
		"react": "^18.3.1"
	},
	"devDependencies": {
		"@prosopo/config": "2.0.3",
		"@vitest/coverage-v8": "^2.0.5",
		"concurrently": "^8.2.2",
		"npm-run-all": "^4.1.5",
