{
	"name": "@prosopo/web-components",
	"version": "2.1.2",
	"description": "Non business logic utilities for web applications",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=20",
		"npm": ">=9"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
