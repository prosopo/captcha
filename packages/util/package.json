{
    "name": "@prosopo/util",
    "version": "0.2.7",
    "author": "PROSOPO LIMITED <info@prosopo.io>",
    "license": "Apache-2.0",
    "private": false,
    "engines": {
        "node": ">=16",
        "npm": "8.9"
    },
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix --config ../../.eslintrc.js",
        "cli": "node ./dist/js/cli.js",
        "test": "NODE_ENV=test vitest --run --coverage"
    },
    "main": "./dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "dependencies": {
        "lodash": "^4.17.21",
        "seedrandom": "^3.0.5"
    },
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/lodash": "^4.14.198",
        "@types/mocha": "^10.0.1",
        "@types/seedrandom": "^3.0.5",
        "@vitest/coverage-v8": "^0.34.3",
        "chai": "^4.3.7",
        "tslib": "2.6.2",
        "typescript": "5.1.6",
        "vitest": "^0.34.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prosopo/captcha.git"
    },
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "description": "Utility package to house all code that is not specific to prosopo. E.g. random number generation, string manipulation, etc.",
    "sideEffects": false
}
