{
	"name": "@prosopo/locale",
	"version": "2.3.2",
	"description": "Prosopo locale library",
	"main": "./dist/index.js",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": ">=9"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		},
		"./locales/de": {
			"import": "./dist/locales/de/translation.json",
			"default": "./dist/locales/de/translation.json"
		},
		"./locales/en": {
			"import": "./dist/locales/en/translation.json",
			"default": "./dist/locales/en/translation.json"
		}
	},
	"types": "./dist/index.d.ts",
	"scripts": {
		"clean": "tsc --build --clean",
		"build": "tsc --build --verbose",
		"build:cjs": "npx vite --config vite.cjs.config.ts build",
		"test": "NODE_ENV=${NODE_ENV:-test}; npx vitest run --config ./vite.test.config.ts"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"dependencies": {
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@prosopo/config": "2.3.2",
		"@vitest/coverage-v8": "2.1.1",
		"del-cli": "6.0.0",
		"dotenv": "16.4.5",
		"npm-run-all": "2.1.0",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
		"typescript": "5.6.2",
		"vite": "5.4.6",
		"vitest": "2.1.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prosopo/captcha.git"
	},
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false
}
