{
	"name": "@prosopo/procaptcha-bundle",
	"version": "3.0.21",
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"type": "module",
	"sideEffects": false,
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		},
		"./components": {
			"import": "./dist/components.js",
			"require": "./dist/components.cjs"
		}
	},
	"types": "dist/index.d.ts",
	"source": "./src/index.tsx",
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV",
		"start": "NODE_ENV=${NODE_ENV:-development}; vite serve --mode $NODE_ENV --host",
		"bundle:webpack": "NODE_ENV=${NODE_ENV:-development}; MODE=${MODE:-$NODE_ENV}; webpack build --config webpack.config.cjs --mode $MODE",
		"serve": "NODE_ENV=${NODE_ENV:-development}; vite serve --config vite.serve.config.ts --mode $NODE_ENV --host",
		"test": "NODE_ENV=${NODE_ENV:-test}; npx vitest run --config ./vite.test.config.ts"
	},
	"browserslist": [
		"> 0.5%, last 2 versions, not dead"
	],
	"dependencies": {
		"@prosopo/dotenv": "3.0.12",
		"@prosopo/locale": "3.1.7",
		"@prosopo/procaptcha-common": "2.8.0",
		"@prosopo/procaptcha-frictionless": "2.7.0",
		"@prosopo/procaptcha-pow": "2.8.0",
		"@prosopo/procaptcha-react": "2.8.0",
		"@prosopo/types": "3.1.0",
		"@prosopo/util": "3.0.10",
		"@prosopo/widget-skeleton": "2.6.8",
		"@prosopo/config": "3.1.7",
		"dotenv": "16.4.5",
		"fast-glob": "3.3.2",
		"jsdom": "25.0.0",
		"react": "18.3.1",
		"@emotion/cache": "11.11.0",
		"@emotion/react": "11.11.1",
		"react-dom": "18.3.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prosopo/captcha.git"
	},
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"description": "",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"devDependencies": {
		"@types/jsdom": "21.1.7",
		"@types/node": "22.10.2",
		"@types/react": "18.3.1",
		"@types/react-dom": "18.3.1",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"overrides": {
		"@types/react": "18.3.1"
	}
}
