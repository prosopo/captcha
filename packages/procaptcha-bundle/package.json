    "main": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "source": "./src/index.tsx",
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose",
        "start": "webpack serve --mode development",
        "build:webpack:prod": "webpack  --config webpack.config.production.js --mode production",
        "build:webpack:prod:copy": "webpack --config webpack.config.production.js --mode production && cp dist/procaptcha_bundle.*.bundle.js ../../demos/client-bundle-example/src",
        "build:webpack:dev": "webpack --config webpack.config.development.js --mode development --watch",
        "build:webpack:dev:copy": "WEBPACK_COPY=true webpack --config webpack.config.development.js --mode development --watch",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix"
    },
    "browserslist": [
        "> 0.5%, last 2 versions, not dead"
    ],
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/material": "^5.14.5",
        "@prosopo/common": "^0.1.17",
        "@prosopo/procaptcha-react": "^0.1.17"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prosopo/captcha.git"
    },
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "description": "",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
