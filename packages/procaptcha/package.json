{
    "name": "@prosopo/procaptcha",
    "version": "0.2.4",
    "author": "PROSOPO LIMITED <info@prosopo.io>",
    "license": "Apache-2.0",
    "main": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "engines": {
        "node": ">=16",
        "npm": "8.9"
    },
    "scripts": {
        "test": "NODE_ENV=test ts-mocha \"tests/**/*.test.ts\" && exit 0",
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix --config ../../.eslintrc.js"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        },
        "./modules/Manager": {
            "import": "./dist/modules/Manager.js",
            "require": "./dist/cjs/modules/Manager.js"
        }
    },
    "dependencies": {
        "@fingerprintjs/fingerprintjs": "^3.3.6",
        "@polkadot/api": "10.9.1",
        "@polkadot/api-contract": "10.9.1",
        "@polkadot/extension-base": "0.46.5",
        "@polkadot/extension-dapp": "0.46.5",
        "@polkadot/extension-inject": "0.46.5",
        "@polkadot/keyring": "12.3.2",
        "@polkadot/rpc-provider": "10.9.1",
        "@polkadot/types": "10.9.1",
        "@polkadot/util": "12.3.2",
        "@polkadot/util-crypto": "12.3.2",
        "@prosopo/api": "0.2.4",
        "@prosopo/common": "0.2.4",
        "@prosopo/contract": "0.2.4",
        "@prosopo/datasets": "0.2.4",
        "@prosopo/types": "0.2.4",
        "@prosopo/util": "0.2.4",
        "axios": "^1.5.0"
    },
    "devDependencies": {
        "mocha": "^10.0.0",
        "ts-mocha": "^10.0.0",
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prosopo/captcha.git"
    },
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "description": "",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "sideEffects": false
}
