{
	"name": "@prosopo/api-route",
	"version": "2.6.35",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"test": "NODE_ENV=${NODE_ENV:-test}; vitest run --config ./vite.test.config.ts",
		"test:watch": "NODE_ENV=${NODE_ENV:-test}; vitest --config ./vite.test.config.ts",
		"test:coverage": "NODE_ENV=${NODE_ENV:-test}; vitest run --coverage --config ./vite.test.config.ts"
	},
	"dependencies": {
		"@prosopo/common": "3.1.27",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@prosopo/config": "3.2.1",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"del-cli": "6.0.0",
		"dotenv": "16.4.5",
		"vitest": "3.2.4"
	},
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
