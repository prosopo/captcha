{
	"name": "@prosopo/database",
	"version": "3.0.12",
	"description": "Prosopo database plugins for provider",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly --force",
		"test": "echo no tests"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/captcha.git"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@prosopo/common": "3.1.1",
		"@prosopo/config": "3.1.2",
		"@prosopo/types": "3.0.5",
		"@prosopo/types-database": "3.0.12",
		"@prosopo/user-access-policy": "3.3.2",
		"@typegoose/auto-increment": "4.13.0",
		"axios": "1.10.0",
		"esbuild": "0.25.6",
		"express": "4.21.2",
		"mongodb": "6.9.0",
		"mongodb-memory-server": "10.0.0",
		"mongoose": "8.13.0",
		"openpgp": "5.11.3",
		"redis": "5.0.0",
		"webpack-dev-server": "5.2.2"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"sideEffects": false
}
