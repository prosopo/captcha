		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@prosopo/common": "2.0.3",
		"@prosopo/config": "2.0.3",
		"@prosopo/types": "2.0.3",
		"@prosopo/types-database": "2.0.3",
		"mongodb": "6.8.1",
		"mongodb-memory-server": "9.4.0",
		"mongoose": "8.5.1"
	},
	"devDependencies": {
		"tslib": "2.6.2",
		"typescript": "5.1.6"
	},
