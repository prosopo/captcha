{
    "name": "@prosopo/database",
    "version": "0.2.1",
    "description": "Prosopo database plugins for provider",
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix --config ../../.eslintrc.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/prosopo/captcha.git"
    },
    "author": "Prosopo Limited",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "dependencies": {
        "@prosopo/common": "0.2.1",
        "@prosopo/types": "0.2.1",
        "@prosopo/types-database": "0.2.1",
        "mongodb": "5.8.0",
        "mongodb-memory-server": "^8.7.2",
        "mongoose": "^7.3.3",
        "zod": "^3.17.9"
    },
    "devDependencies": {
        "tslib": "^2.6.2",
        "typescript": "^5.1.6"
    },
    "sideEffects": false
}
