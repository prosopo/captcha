{
	"name": "@prosopo/database",
	"version": "3.6.1",
	"description": "Prosopo database plugins for provider",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/captcha.git"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"dependencies": {
		"@polkadot/util": "13.5.7",
		"@prosopo/common": "3.1.27",
		"@prosopo/locale": "3.1.27",
		"@prosopo/redis-client": "1.0.12",
		"@prosopo/types": "3.7.1",
		"@prosopo/types-database": "4.1.1",
		"@prosopo/user-access-policy": "3.5.34",
		"make-dir": "3.1.0",
		"mongodb": "6.15.0",
		"mongodb-memory-server": "10.3.0",
		"mongoose": "8.13.0"
	},
	"devDependencies": {
		"@prosopo/config": "3.1.27",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.4.1",
		"vitest": "3.2.4"
	},
	"sideEffects": false
}
