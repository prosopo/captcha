{
    "name": "@prosopo/database",
    "version": "0.2.40",
    "description": "Prosopo database plugins for provider",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18",
        "npm": ">=9"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "clean": "tsc --build --clean",
        "build": "tsc --build --verbose tsconfig.json",
        "build:cjs": "npx vite --config vite.cjs.config.ts build",
        "eslint": "npx eslint . --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore",
        "eslint:fix": "npm run eslint -- --fix",
        "prettier": "npx prettier . --check --no-error-on-unmatched-pattern --ignore-path ../../.eslintignore",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "npm run eslint && npm run prettier",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/prosopo/captcha.git"
    },
    "author": "Prosopo Limited",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/prosopo/captcha/issues"
    },
    "homepage": "https://github.com/prosopo/captcha#readme",
    "dependencies": {
        "@polkadot/util": "^12.5.1",
        "@prosopo/captcha-contract": "0.2.40",
        "@prosopo/common": "0.2.40",
        "@prosopo/config": "0.2.40",
        "@prosopo/types": "0.2.40",
        "@prosopo/types-database": "0.2.40",
        "mongodb": "5.8.0",
        "mongodb-memory-server": "^8.7.2",
        "mongoose": "^7.3.3"
    },
    "devDependencies": {
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "sideEffects": false
}
