{
	"name": "@prosopo/client-bundle-example",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"type": "module",
	"engines": {
		"node": "20",
		"npm": "11.5.2"
	},
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly",
		"start": "NODE_ENV=${NODE_ENV:-development}; vite serve ./src --port 9232 --config vite.config.ts --mode=$NODE_ENV"
	},
	"version": "2.9.9",
	"dependencies": {
		"@prosopo/config": "3.1.6"
	},
	"devDependencies": {
		"@prosopo/dotenv": "3.0.11",
		"@types/node": "22.5.5",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	}
}