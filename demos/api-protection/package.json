{
	"name": "@prosopo/api-protection-demo",
	"version": "1.0.0",
	"description": "Demo showcasing API protection using Procaptcha for hotel booking platforms",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
		"dev:frontend": "vite --port 9233",
		"dev:backend": "tsx watch src/backend/server.ts",
		"build": "npm run build:frontend",
		"build:frontend": "tsc && vite build",
		"build:backend": "tsc src/backend/server.ts --outDir dist/backend --module commonjs --target es2020",
		"preview": "vite preview",
		"start": "node dist/backend/server.js",
		"start:backend": "tsx src/backend/server.ts",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@prosopo/dotenv": "3.0.32",
		"@prosopo/react-procaptcha-wrapper": "1.1.43",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.0",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"postcss": "^8.4.32",
		"tailwindcss": "^3.3.6",
		"tsx": "^4.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.8"
	},
	"keywords": [
		"procaptcha",
		"captcha",
		"api-protection",
		"web-scraping",
		"security",
		"demo",
		"hotel-booking",
		"react",
		"typescript"
	]
}
