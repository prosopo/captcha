{
	"name": "@prosopo/provider-mock",
	"version": "2.8.2",
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"scripts": {
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV"
	},
	"dependencies": {
		"@prosopo/api-express-router": "3.0.10",
		"@prosopo/common": "3.1.5",
		"@prosopo/database": "3.0.19",
		"@prosopo/dotenv": "3.0.10",
		"@prosopo/locale": "3.1.5",
		"@prosopo/provider": "3.6.2",
		"@prosopo/types": "3.0.9",
		"@prosopo/types-database": "3.0.19",
		"@prosopo/config": "3.1.5",
		"cors": "2.8.5",
		"express": "4.21.2",
		"mongoose": "8.13.0"
	},
	"devDependencies": {
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/provider.git"
	},
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
