{
	"name": "@prosopo/provider-mock",
	"version": "2.8.63",
	"author": "PROSOPO LIMITED <info@prosopo.io>",
	"license": "Apache-2.0",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"engines": {
		"node": "^24",
		"npm": "^11"
	},
	"scripts": {
		"build": "npm run build:cross-env -- --mode ${NODE_ENV:-development}",
		"build:cross-env": "vite build --config vite.esm.config.ts",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"bundle": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.config.ts --mode $NODE_ENV"
	},
	"dependencies": {
		"@prosopo/api-express-router": "3.0.60",
		"@prosopo/common": "3.1.28",
		"@prosopo/database": "3.9.4",
		"@prosopo/dotenv": "3.0.33",
		"@prosopo/locale": "3.1.28",
		"@prosopo/provider": "3.20.3",
		"@prosopo/types": "3.11.0",
		"@prosopo/types-database": "4.4.4",
		"cors": "2.8.5",
		"express": "4.21.2",
		"mongoose": "8.13.0"
	},
	"devDependencies": {
		"@prosopo/config": "3.3.0",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.4.1",
		"vitest": "3.2.4"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/prosopo/provider.git"
	},
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha#readme",
	"sideEffects": false
}
