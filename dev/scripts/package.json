{
	"name": "@prosopo/scripts",
	"version": "3.1.5",
	"description": "Dev scripts for working with prosopo packages",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"type": "module",
	"engines": {
		"node": ">=22",
		"npm": "10.9.2"
	},
	"scripts": {
		"build": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-development}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --project tsconfig.types.json",
		"cli": "node dist/cli/index.js",
		"setup": "node dist/cli/index.js setup"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"dependencies": {
		"@iarna/toml": "2.2.5",
		"@polkadot/api": "10.13.1",
		"@polkadot/api-contract": "10.13.1",
		"@polkadot/util": "12.6.2",
		"@polkadot/util-crypto": "12.6.2",
		"@prosopo/cli": "3.3.0",
		"@prosopo/common": "3.1.12",
		"@prosopo/config": "3.1.12",
		"@prosopo/datasets": "3.0.26",
		"@prosopo/dotenv": "3.0.17",
		"@prosopo/env": "3.2.5",
		"@prosopo/keyring": "2.8.19",
		"@prosopo/locale": "3.1.12",
		"@prosopo/provider": "3.9.0",
		"@prosopo/types": "3.2.0",
		"@prosopo/types-env": "2.7.30",
		"@prosopo/util": "3.0.15",
		"@prosopo/util-crypto": "13.5.14",
		"@prosopo/workspace": "3.1.5",
		"dotenv": "16.4.5",
		"fast-glob": "3.3.2",
		"fs-extra": "11.2.0",
		"rollup": "4.50.1",
		"yargs": "17.7.2"
	},
	"overrides": {
		"@polkadot/keyring": "12.6.2"
	},
	"devDependencies": {
		"@types/fs-extra": "11.0.4",
		"@types/node": "22.10.2",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "^4.8.1",
		"typescript": "5.6.2",
		"vite": "6.3.6",
		"vitest": "3.0.9"
	}
}
