{
	"name": "@prosopo/lint",
	"version": "2.7.5",
	"description": "",
	"private": true,
	"type": "module",
	"scripts": {
		"clean": "del-cli --verbose dist tsconfig.tsbuildinfo",
		"build": "NODE_ENV=${NODE_ENV:-production}; vite build --config vite.esm.config.ts --mode $NODE_ENV",
		"build:tsc": "tsc --build --verbose",
		"build:cjs": "NODE_ENV=${NODE_ENV:-production}; vite build --config vite.cjs.config.ts --mode $NODE_ENV",
		"typecheck": "tsc --build --declaration --emitDeclarationOnly --force",
		"test": "echo no tests",
		"engines": "node dist/engines.js",
		"refs": "node dist/refs.js",
		"workflowNames": "node dist/workflowNames.js",
		"scripts": "node dist/scripts.js",
		"license": "node dist/license.js check",
		"license:fix": "node dist/license.js license",
		"redirects": "node dist/redirects.js",
		"redirects:fix": "node dist/redirects.js fix",
		"tsconfig:includes": "node dist/tsconfigIncludes.js",
		"tsconfig:includes:fix": "node dist/tsconfigIncludes.js --fix"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "22.5.5",
		"@vitest/coverage-v8": "3.0.9",
		"concurrently": "9.0.1",
		"del-cli": "6.0.0",
		"npm-run-all": "4.1.5",
		"tslib": "2.7.0",
		"tsx": "4.20.3",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "3.0.9"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"engines": {
		"node": "20",
		"npm": "10.8.2"
	},
	"dependencies": {
		"@prosopo/util": "3.0.3",
		"@typegoose/auto-increment": "4.13.0",
		"axios": "1.10.0",
		"esbuild": "0.25.6",
		"express": "4.21.2",
		"fast-glob": "3.3.2",
		"openpgp": "5.11.3",
		"webpack-dev-server": "5.2.2",
		"yargs": "17.7.2",
		"zod": "3.23.8",
		"@prosopo/config": "3.1.1"
	}
}
