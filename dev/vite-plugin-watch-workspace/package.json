{
	"name": "@prosopo/vite-plugin-watch-workspace",
	"version": "2.1.3",
	"description": "Vite plugin for watching and rebuilding external files",
	"main": "./dist/index.js",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"scripts": {
		"test": "echo \"No test specified\"",
		"clean": "tsc --build --clean",
		"build": "tsc --build --verbose",
		"build:cjs": "npx vite --config vite.cjs.config.ts build"
	},
	"engines": {
		"node": ">=20",
		"npm": ">=9"
	},
	"author": "Prosopo Limited",
	"license": "Apache-2.0",
	"dependencies": {
		"@prosopo/config": "2.1.3",
		"debug": "4.3.7",
		"esbuild": "0.23.1",
		"fast-glob": "3.3.2"
	},
	"devDependencies": {
		"@types/debug": "4.1.12",
		"@vitest/coverage-v8": "2.1.1",
		"concurrently": "9.0.1",
		"npm-run-all": "4.1.5",
		"rimraf": "6.0.1",
		"tslib": "2.7.0",
		"tsx": "4.19.1",
		"typescript": "5.6.2",
		"vite": "5.4.6",
		"vitest": "2.1.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prosopo/captcha.git"
	},
	"bugs": {
		"url": "https://github.com/prosopo/captcha/issues"
	},
	"homepage": "https://github.com/prosopo/captcha/blob/main/dev/vite-plugin-watch-workspace/README.md",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false
}
